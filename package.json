{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "NODE_ENV=testing jest --forceExit --runInBand --detectOpenHandles --watch",
    "deploy:prod": "prisma deploy --env-file prisma_prod.env",
    "deploy:dev": "prisma deploy --env-file prisma_dev.env",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "dotenv": "6.0.0",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "nodemailer": "^4.6.8",
    "nodemon": "1.18.4",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.18.1",
    "prisma-binding": "2.1.6"
  },
  "license": "MIT",
  "repository": {
    "url": "notsure.com"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1"
  }
}
